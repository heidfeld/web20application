extends menu

block contentMenu
  h3 Session ID: #{idSesji}
  h4 Page views: #{odwiedziny}
  p req.session.cookie (Cookies):
    ul
      for val, key in ciasteczko
        li #{key}: #{val}
  p req.session.cookie.data:
    ul
      for val, key in data
        li #{key}: #{val}
  if passport
    p req.session.passport
      ul
        for val, key in passport
          li #{key}: #{val}
